# {{ ansible_managed }}

configDirectory={{ fspath_pihole_config }}
installDirectory={{ fspath_pihole }}
PIHOLE_INTERFACE={{ network_interface }}
IPV4_ADDRESS={{ ipv4_address }}/24
IPV6_ADDRESS={{ ipv6_address }}
PIHOLE_DNS_1={{ dns_servers[0] }}
PIHOLE_DNS_2={{ dns_servers[1] }}
QUERY_LOGGING=true
INSTALL_WEB=true
INSTALL_WEB_SERVER=true
INSTALL_WEB_INTERFACE=true
LIGHTTPD_ENABLED=true
CACHE_SIZE=10000
BLOCKING_ENABLED=true
ADMIN_EMAIL=
WEBUIBOXEDLAYOUT=boxed
WEBTHEME=default-darker
DHCP_ACTIVE=true
DHCP_START=192.168.1.2
DHCP_END=192.168.1.254
DHCP_ROUTER=192.168.1.1
DHCP_LEASETIME=24
PIHOLE_DOMAIN=lan
DHCP_IPv6=false
DHCP_rapid_commit=false
